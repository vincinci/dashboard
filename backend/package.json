{
  "name": "iwanyu-backend",
  "version": "1.0.0",
  "description": "Backend API for Iwanyu vendor dashboard",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "seed": "node scripts/seed.js",
    "postinstall": "prisma generate",
    "build": "prisma generate && prisma db push",
    "db:init": "node scripts/init-production-db.js",
    "db:simple": "node scripts/simple-init.js",
    "db:reset": "node scripts/reset-db.js",
    "db:fix": "node scripts/quick-fix-schema.js",
    "db:migrate": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@stackframe/stack": "^2.8.12",
    "@vercel/blob": "^1.1.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.3",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0"
  }
}
